# Supabase Configuration
# Get these values from your Supabase project settings
# https://app.supabase.com/project/_/settings/api

NEXT_PUBLIC_SUPABASE_URL=your-project-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# PayMongo Configuration
# Get these values from your PayMongo Dashboard
# https://dashboard.paymongo.com/developers/api-keys

# Production Keys (use in production/live environment)
PAYMONGO_PUBLIC_KEY=pk_live_your_public_key_here
PAYMONGO_SECRET_KEY=sk_live_your_secret_key_here

# Test Keys (use in development/preview environment)
PAYMONGO_PUBLIC_KEY_TEST=pk_test_your_test_public_key_here
PAYMONGO_SECRET_KEY_TEST=sk_test_your_test_secret_key_here
